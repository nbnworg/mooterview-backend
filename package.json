{
  "name": "mooterview-backend",
  "version": "1.0.0",
  "description": "mooterview backend development using express, serverless and aws",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc",
    "deploy:sandbox": "npm run build && serverless deploy --stage sandbox",
    "remove:sandbox": "serverless remove --stage sandbox",
    "deploy:production": "npm run build && serverless deploy --stage production",
    "remove:production": "serverless remove --stage production",
    "test": "mocha -r ts-node/register \"test/**/*.test.ts\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-amplify/core": "^6.12.0",
    "@aws-amplify/storage": "^6.8.4",
    "@aws-sdk/client-cognito-identity-provider": "^3.830.0",
    "@aws-sdk/client-dynamodb": "^3.772.0",
    "@aws-sdk/lib-dynamodb": "^3.772.0",
    "@langchain/core": "^0.3.57",
    "@langchain/openai": "^0.5.11",
    "aws-sdk": "^2.1692.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "langchain": "^0.3.27",
    "mooterview-server": "^0.0.3",
    "serverless-http": "^3.2.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.147",
    "@types/aws-sdk": "^0.0.42",
    "@types/chai": "^4.3.11",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/mocha": "^10.0.6",
    "@types/node": "^22.15.32",
    "@types/sinon": "^10.0.15",
    "chai": "^4.4.1",
    "esbuild": "^0.25.1",
    "mocha": "^10.4.0",
    "rimraf": "^6.0.1",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
